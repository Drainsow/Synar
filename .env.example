# Discord bot token (never commit the real value)
DISCORD_TOKEN=your_discord_bot_token

# Development server ID (for testing purposes)
DEV_GUILD_ID=your_dev_guild_id

# Environment mode:
# DEV = local development (verbose logging, test server)
# PROD = production (clean logs, real servers)
ENV=DEV

# Logging verbosity:
# DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=DEBUG

# Command syncing:
# SYNC_COMMANDS: True enables syncing on startup. False disables it.
# CLEAR_COMMANDS: True enables clearing the command cache on startup. False disables it.
SYNC_COMMANDS=False
CLEAR_COMMANDS=False